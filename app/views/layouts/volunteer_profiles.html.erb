<% content_for :content do %>
  <%= render 'volunteer/partials/main_header' %>
  <%= yield %>

  <% unless @current_volunteer %>
    <%= render 'home/modals/registration' %>
    <%= render 'home/modals/confirmation' %>
    <%= render 'home/modals/thank_you' %>
  <% end %>
<% end %>

<%= render template: "layouts/application" %>